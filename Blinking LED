//Blinking LED

const int LED_Pin = 13;

const int Time_Delay = 1000;

void setup()
{
  pinMode(LED_Pin, OUTPUT);
}

void loop()
{
  digitalWrite(LED_Pin, HIGH);
  delay(Time_Delay);

  digitalWrite(LED_Pin, LOW);
  delay(Time_Delay);
}


/*
LED Connections -
Anode     - Digital Pin 11
Cathode   - 220Î© Resistor from Ground
*/
