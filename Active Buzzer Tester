//Active Buzzer Tester

const int Buzzer_Pin = 4;

const int Time_Delay = 1000;

void setup()
{
  pinMode(Buzzer_Pin, OUTPUT);
}

void loop()
{
  digitalWrite(Buzzer_Pin, HIGH);
  delay(Time_Delay);

  digitalWrite(Buzzer_Pin, LOW);
  delay(Time_Delay);
}

/*
Connections -
Buzzer Anode    - Digital Pin 4
Buzzer Cathode  - Ground
*/
