//Arduino Uno R3 Board Tester

const int Time_Delay = 1000;

void setup()
{
  pinMode(LED_BUILTIN, OUTPUT);
}

void loop()
{
  digitalWrite(LED_BUILTIN, HIGH);
  delay(Time_Delay);

  digitalWrite(LED_BUILTIN, LOW);
  delay(Time_Delay);
}
